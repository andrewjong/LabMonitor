{
  "name": "labmonitor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "cd client && yarn start",
    "server": "yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "PORT=3001 node ./bin/www",
    "test": "jest",
    "start-dummy-data": "node make-dummy-data.js -c start",
    "stop-dummy-data": "node make-dummy-data.js -c stop"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "jest": "^22.0.5",
    "nodemon": "^1.14.10"
  }
}
